        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_frontTotalToe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/front_total_toe" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_leftFrontCamber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/left_front_camber" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_rightFrontCamber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/right_front_camber" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

