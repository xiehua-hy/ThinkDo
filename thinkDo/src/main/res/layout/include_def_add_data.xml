        android:layout_height="wrap_content"
        skd:DBarTitle="@string/rear_total_toe" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_leftRearCamber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/left_rear_camber" />

