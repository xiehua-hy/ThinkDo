    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_rightCaster"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/right_caster" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_leftKpi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
