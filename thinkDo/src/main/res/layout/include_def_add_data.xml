    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_front_wheel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/front_wheel" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#CCC" />

    <com.thinkdo.view.BarDefAdd
        android:id="@+id/barDefAdd_rearWheel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        skd:DBarTitle="@string/rearWheel" />

