<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:skd="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/divider">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/print_title_margin"
            android:text="@string/test_result"
            android:textSize="@dimen/print_title_font_size" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_print_form"
            android:orientation="vertical"
            android:padding="2dp">

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/parameters" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/front_total_toe" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/left_front_toe" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/right_front_toe" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/left_front_camber" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/right_front_camber" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/left_caster" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/right_caster" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/left_kpi" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/right_kpi" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/rear_total_toe" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/left_rear_toe" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/right_rear_toe" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/left_rear_camber" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/right_rear_camber" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/thrust_angle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/wheelbase_diff" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/wheel_diff" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/left_include_angle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/right_include_angle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/left_turn_angle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintTitle="@string/right_turn_angle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/left_max_turn_angle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider" />

            <com.thinkdo.view.BarPrint
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                skd:PrintContentBg="@color/bar_holo"
                skd:PrintTitle="@string/right_max_turn_angle" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="32dp"
            android:layout_marginTop="24dp"
            android:gravity="center">

            <Button
                android:id="@id/btn_save"
                style="?android:attr/buttonBarButtonStyle"
                android:layout_width="@dimen/print_btn_width"
                android:layout_height="@dimen/print_btn_height"
                android:layout_marginEnd="64dp"
                android:layout_marginRight="64dp"
                android:background="@drawable/selector_next_btn"
                android:text="@string/save"
                android:textColor="@color/font_black"
                android:textSize="@dimen/print_title_font_size" />

            <Button
                android:id="@+id/btn_print"
                style="?android:attr/buttonBarButtonStyle"
                android:layout_width="@dimen/print_btn_width"
                android:layout_height="@dimen/print_btn_height"
                android:background="@drawable/selector_next_btn"
                android:text="@string/print"
                android:textColor="@color/font_black"
                android:textSize="@dimen/print_title_font_size" />
        </LinearLayout>
    </LinearLayout>
</ScrollView>