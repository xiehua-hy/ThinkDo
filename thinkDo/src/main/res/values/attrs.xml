<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BarItem">
        <attr name="barIcon" format="reference" />
        <attr name="barNextIcon" format="reference" />
        <attr name="barTitle" format="string" />
        <attr name="barTextColor" format="color" />
        <attr name="barBg" format="reference" />
    </declare-styleable>

    <declare-styleable name="ButtonRaise">
        <attr name="buttonTextInUp" format="string" />
        <attr name="buttonTextInDown" format="string" />
    </declare-styleable>

    <declare-styleable name="WindowRealTime">
        <attr name="RealTimeTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="BarPrint">
        <attr name="PrintTitle" format="string" />
        <attr name="PrintContentBg" format="color" />
    </declare-styleable>

    <declare-styleable name="WindowSpecial">
        <attr name="specialTitle" format="string" />
        <attr name="specialSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="BarReferData">
        <attr name="referTitle" format="string" />
        <attr name="referLeftMinText" format="string" />
        <attr name="referLeftMidText" format="string" />
        <attr name="referLeftMaxText" format="string" />
        <attr name="referRightMinText" format="string" />
        <attr name="referRightMidText" format="string" />
        <attr name="referRightMaxText" format="string" />
        <attr name="referTitleFontColor" format="color" />
        <attr name="referDefaultBg" format="boolean" />
    </declare-styleable>
</resources>